  ConstantOp,
  Conv2dOp,
  ConvTranspose2dOp,
  CpuOp,
  DeallocateOp,
  EltwiseBinaryCompositeOp,
  EltwiseBinaryOp,
  EltwiseQuantizationOp,
  EltwiseTernaryWhereOp,
  EltwiseUnaryCompositeOp,
  EltwiseUnaryOp,
  EmbeddingBackwardOp,
  EmbeddingOp,
  EmptyOp,
  EndTraceCaptureOp,
  ExecuteTraceOp,
  FillCacheOp,
  FromDeviceOp,
  FullOp,
  FuncCallOp,
