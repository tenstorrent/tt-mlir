table Operation {
  type: OpType;
  debug_info: string;
  loc_info: string;
}

table Program {
  name: string;
  inputs: [TensorRef];
  outputs: [TensorRef];
  operations: [Operation];
  dylibs: [DynamicLib];
  debug_info: DebugInfo;
  // Flag to indicate if this program is intended to be called or only for internal use (e.g. hoisted const-eval func).
  private: bool;
  mesh_shape: Dim2d;
}
