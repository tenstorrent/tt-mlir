+++ b/shardy/integrations/python/ir/CMakeLists.txt
@@ -0,0 +1,45 @@
+# Shardy MLIR Module
+
+set(SHARDY_ROOT_DIR "${TTMLIR_TOOLCHAIN_DIR}/src/shardy/shardy/integrations/python/ir")
+
+# Rename _sdy_enums_gen in sdy.py
+file(READ "${SHARDY_ROOT_DIR}/sdy.py" FILE_CONTENTS)
+string(REPLACE "enums" "enum" FILE_CONTENTS "${FILE_CONTENTS}")
+file(WRITE "${SHARDY_ROOT_DIR}/sdy.py" "${FILE_CONTENTS}")
+
+if (EXISTS ${SHARDY_ROOT_DIR}/dialects)
+  file(REMOVE_RECURSE ${SHARDY_ROOT_DIR}/dialects)
+endif()
+file(MAKE_DIRECTORY ${SHARDY_ROOT_DIR}/dialects)
+FILE(COPY ${SHARDY_ROOT_DIR}/sdy.py DESTINATION ${SHARDY_ROOT_DIR}/dialects)
+FILE(COPY ${SHARDY_ROOT_DIR}/sdy_ops.td DESTINATION ${SHARDY_ROOT_DIR}/dialects)
+FILE(COPY ${SHARDY_ROOT_DIR}/sdy_enums.td DESTINATION ${SHARDY_ROOT_DIR}/dialects)
+
+declare_mlir_python_sources(SdyPythonSources)
+declare_mlir_python_sources(SdyPythonSources.Dialects
+  ADD_TO_PARENT SdyPythonSources
+)
+
+declare_mlir_dialect_python_bindings(
+  ADD_TO_PARENT SdyPythonSources.Dialects
