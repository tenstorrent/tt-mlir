#!/bin/bash

# Confirm python3 exists
if ! command -v python3 &> /dev/null; then
    echo "Error: python3 could not be found"
    exit 1
fi

# Confirm ttnn is installed
if ! python3 -c "import ttnn" &> /dev/null; then
    echo "Error: ttnn could not be imported"
    exit 1
fi

# Run main.py
python3 main.py
