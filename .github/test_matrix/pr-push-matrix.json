[
  { "runs-on": ["n150","n300"],
        "image": ["speedy","tracy"],
            "type": "unit" },

  { "runs-on": ["n150","n300","llmbox"],
        "image": "speedy",
            "type": "ttrt", "path": "Silicon", "flags": "perf", "args": "--non-zero" },

  { "runs-on": ["n150","n300","llmbox"],
        "image": "tracy",
            "type": "ttrt", "path": "Silicon/TTNN/$RUNS_ON/perf", "flags": "perf"},

  { "runs-on": "n150",
        "image": "tracy",
            "type": "pytest",
                "path": ["tools/ttrt/test", "test/python/op_by_op"] },

  { "runs-on": "n150",   "image": "tracy",  "type": "pytest", "path": "tools/explorer/test/run_tests.py", "flags": "--force-reinstall $BUILD_DIR/bin/wheels/tt_adapter*.whl $BUILD_DIR/bin/wheels/ai_edge_model_explorer*.whl"},
  { "runs-on": "n150",   "image": "tracy",  "type": "pytest", "path": "runtime/tools/chisel/test", "flags": "-e runtime/tools/chisel/"},
  { "runs-on": "n300",   "image": "tracy",  "type": "pytest", "path": "runtime/test/ttnn/python/multi_device"},

  { "runs-on": ["n150","llmbox","n300","p150"],
        "image": "tracy",
            "type": "builder", "path": "test/python/golden", "args": "not run_error and not fails_golden", "flags": "run-ttrt"},

  { "runs-on": "n150",   "image": "speedy", "type": "builder", "path": "test/python/golden/optimizer", "args": "not run_error and not fails_golden", "flags": "run-ttrt require-opmodel"},
  { "runs-on": "n150",   "image": "tracy",  "type": "ttrt", "path": "Silicon/TTNN/n150/optimizer", "flags": "perf"},

  { "runs-on": "n150",   "image": "speedy", "type": "op_model", "path": "Silicon/TTNN/n150/optimizer", "args": "--non-zero" },
  { "runs-on": "n150",   "image": "tracy",  "type": "ttnn_runtime", "path": "runtime/test/ttnn/python/device_agnostic" },
  { "runs-on": "n300",   "image": "tracy",  "type": "runtime_debug" },
  { "runs-on": "n150",   "image": "speedy", "type": "ttnn_standalone" },
  { "runs-on": "n150",   "image": "tracy",  "type": "emitc", "path": "EmitC" },
  { "runs-on": "n150",   "image": "tracy",  "type": "pykernel" }
]
